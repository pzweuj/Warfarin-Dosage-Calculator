{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "warfarin-dosage-calculator",
  "version": "0.0.3",
  "identifier": "com.wardoscal.dev",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "warfarin-dosage-calculator",
        "width": 1300,
        "height": 760,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/favicon.ico"
    ]
  }
}
